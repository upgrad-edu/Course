        "model = spacy.load(\"en_core_web_sm\")\n",
        "\n",
        "def get_token(sent_list):\n",
        "  for sentence in sent_list :\n",
        "     tokens = model(sentence)\n",
        "     for token in tokens:\n",
        "       print(token, \"--\", token.pos_, \"--\", token.tag_)\n"
      ],
